---
layout: none
---
[
  {% assign workshops = site.data.workshop_list 
    | sort: "title" 
  %}

  {% for workshop in workshops %}
    {

      "title"      : "{{ workshop.title }}",
      "description": "{{ workshop.description }}",
      "url"        : "{{ workshop.url }}",
      "tags"       : "{{ workshop.tags }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]