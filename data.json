---
layout: none
---
  {
  {% assign workshops = site.data.workshop_list 
    | sort: "title" 
  %}
  {% for workshop in workshops %}
    "{{ workshop.title }}": { {% assign splitTags = workshop.tags | split: ", " %}
      "title"      : "{{ workshop.title }}",
      "description": "{{ workshop.description }}",
      "url"        : "{{ workshop.url }}",
      "tags"       : "{{ workshop.tags }}"
      {% comment %}[{% for tag in splitTags %}"{{tag}}"{% unless forloop.last %}, {% endunless %}{% endfor %}]{% endcomment %}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}